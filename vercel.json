{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node",
        "config": { 
          "maximumFileSizeToCacheInBytes": 10000000,
          "installCommand": "npm install --legacy-peer-deps"
        }
      }
    ],
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }